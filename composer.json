{
    "name": "thrashzone13/visitor",
    "type": "library",
    "description": "Simple implementation of visitor design pattern",
    "keywords": [
        "thrashzone13",
        "visitor"
    ],
    "homepage": "https://github.com/thrashzone13/visitor",
    "license": "MIT",
    "authors": [
        {
            "name": "Reza Rabbani",
            "email": "reza.rabbani.369@gmail.com",
            "homepage": "https://github.com/thrashzone13",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "11.2.8",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Thrashzone13\\Visitor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thrashzone13\\Visitor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
